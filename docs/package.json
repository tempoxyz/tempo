{
  "name": "docs",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "NODE_OPTIONS=\"--max-old-space-size=16384\" vocs build --searchIndex",
    "check": "biome check . --write --unsafe",
    "check:ci": "biome check . --diagnostic-level=error",
    "check:types": "tsc --noEmit",
    "dev": "vocs dev",
    "gen:specs": "node scripts/gen-specs.ts",
    "prepare": "simple-git-hooks",
    "preview": "vocs preview"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-beta.7",
    "@monaco-editor/react": "^4.7.0",
    "@tanstack/react-query": "^5.90.5",
    "@vercel/analytics": "^1.6.1",
    "@vercel/node": "^5.5.10",
    "@vercel/speed-insights": "^1.3.1",
    "@wagmi/core": "^3.1.0",
    "cva": "1.0.0-beta.4",
    "monaco-editor": "^0.55.1",
    "nuqs": "^2.8.2",
    "ox": "^0.10.2",
    "posthog-js": "^1.309.1",
    "prool": "^0.2.1",
    "react": "latest",
    "react-dom": "latest",
    "react-router": "7.9.4",
    "sonner": "^2.0.7",
    "sql-formatter": "^15.6.10",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "tempo.ts": "~0.13.0",
    "viem": "https://pkg.pr.new/viem@e5bd0c8",
    "vocs": "^1.4.1",
    "wagmi": "https://pkg.pr.new/wagmi@017b6c6",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@iconify/json": "^2.2.403",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/node": "latest",
    "@types/react": "latest",
    "monaco-editor-core": "^0.55.1",
    "simple-git-hooks": "^2.13.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-icons": "^22.5.0"
  },
  "simple-git-hooks": {
    "pre-commit": "bun run check && (git diff --cached --name-only | grep -q '^specs/' && bun run gen-specs || true)"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
